<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4020.6">
  <POU Name="FB_CycleInit" Id="{5ed5b3ed-bbb0-46c6-af0d-96cc7ab0e3b1}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_CycleInit
VAR_INPUT
	xStart : BOOL;
	xStop : BOOL;
END_VAR
VAR_OUTPUT
	fbRtrigStart:R_trig;
	xBusy:BOOL;
	iEtape:INT;
END_VAR
VAR
	fbEtape:FB_Etape;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[fbEtape.mActualisation(stParam.stStatus.xModePasAPas,fbBtn.xNextPas);

fbRtrigStart(clk:=fbBtn.xStart);


CASE fbEtape.iNo OF
	0:fbEtape.psMess:='Attendre - Pret';
		IF xStart THEN
			fbEtape.piVal:=99;
		END_IF
	99: fbEtape.psMess:='Reset sur les Actionneurs';
		fbEtape.piSVal:=100;
		
	100:fbEtape.psMess:='Initialiser les positions';
		IF  fbAct1.xInInPos AND fbAct2.xInInPos AND NOT fbAct3.xInInPos THEN
			fbEtape.piSVal:=999;
		END_IF
	
	999:fbEtape.psMess:='Fin Cycle Init - Attente de Stop';
		IF (xStop) THEN
		fbEtape.piSVal:=0;
	END_IF
END_CASE

xBusy:=fbEtape.iNo<>0;
iEtape:=fbEtape.iNo;]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>